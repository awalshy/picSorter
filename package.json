{
  "name": "picsorter-cli",
  "description": "Picture Sorter CLI",
  "version": "0.0.0",
  "author": "Arthur Walsh",
  "bin": {
    "picsorter-cli": "./bin/run"
  },
  "bugs": "https://github.com/./awalshy/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "cli-ux": "^6.0.6",
    "exifr": "^7.1.3",
    "inquirer": "^8.2.0",
    "node-notifier": "^10.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/exif": "^0.6.3",
    "@types/inquirer": "^8.1.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.2",
    "@types/node-notifier": "^8.0.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2.0.0-main.10",
    "rimraf": "^3.0.2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/./awalshy",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "picsorter-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "Events": {
        "description": "List or Generate Event Files"
      },
      "Organize": {
        "description": "Sort or Reorganize folders"
      },
      "Peuple": {
        "description": "Manage Events on Peuple"
      }
    }
  },
  "repository": "./awalshy",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
